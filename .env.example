# Frontend .env.example
# Copy this file to .env and fill in your values

# AI Proxy Configuration
REACT_APP_USE_AI_PROXY=true

# URL to your backend proxy endpoint
# During development with a local backend, this would be http://localhost:3001/api/chat
# When deployed, this could be /api/chat (relative to your domain)
REACT_APP_AI_PROXY_URL=http://localhost:3001/api/chat
